/* Five Classic Tape Techniques */

format channels = mono
array lisa = read("/Users/luisfelipe/Desktop/Scandal/wav/stereoLisa.wav", channels)
array intro = loop(lisa, 0, 4410, 9) /* Arguments: array, start sample, end sample, count */
array coda1 = reverse(intro)
array coda2 = speed(coda1, 0.7)
array final = splice(intro, coda1, coda2)
play(delay(final, 700, 0.5, 0.5), channels) /* Arguments: array, milliseconds, feedback, mix */
